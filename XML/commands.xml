		<desc>I can send a text for you!</desc>
		<others>
			<alias>send text</alias>
		</others>
	</command>
	<command>
		<keyword>list</keyword>
		<desc>Lists all the commands with their aliases and descriptions</desc>
		<others>
			<alias>list commands</alias>
			<alias>commands</alias>
			<alias>l</alias>
			<alias>display cmds</alias>
